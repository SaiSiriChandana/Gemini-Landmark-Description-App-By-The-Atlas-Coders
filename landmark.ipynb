        "import gradio as gr\n",
        "\n",
        "def detect_and_describe(image):\n",
        "    \"\"\"Detects a landmark and generates a description.\"\"\"\n",
        "    landmark = detect_landmark_gemini(image)  # Detect the landmark\n",
        "    description = generate_description(landmark)  # Generate description\n",
        "    return landmark, description  # Return both results\n",
        "\n",
        "# Gradio Interface\n",
        "iface = gr.Interface(\n",
        "    fn=detect_and_describe,  # Updated function\n",
        "    inputs=gr.Image(type=\"pil\"),  # Input is an image\n",
        "    outputs=[\"text\", \"text\"],  # Outputs are landmark name & description\n",
        "    title=\"Landmark Detection & Description\",\n",
        "    description=\"Upload an image, and the AI will detect the landmark and provide a historical description.\"\n",
        ")\n",
        "\n",
        "# Launch the Web App\n",
        "iface.launch()"
