        "    model = genai.GenerativeModel(\"gemini-1.5-flash\")  # Use the latest model\n",
        "\n",
        "    with open(image_path, \"rb\") as image_file:\n",
        "        image_data = image_file.read()\n",
        "\n",
        "    image = Image.open(io.BytesIO(image_data))  # Convert bytes to PIL Image\n",
        "\n",
        "    prompt = \"Identify the famous landmark in this image and provide its name.\"\n",
        "\n",
        "    response = model.generate_content([prompt, image])  # Pass the PIL Image object\n",
        "\n",
        "    return response.text if response else \"Landmark not recognized\"\n",
        "\n",
        "# Example Usage\n",
        "image_path = \"charminar.jpg\"  # Replace with your image path\n",
        "landmark = detect_landmark_gemini(image_path)\n",
